{
  "name": "galko",
  "version": "1.0.0",
  "description": "Matching cards game",
  "private": true,
  "homepage": "https://bakuzan.github.io/galko/",
  "main": "dist/index.html",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "webpack --env production",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "lint-staged",
    "start": "webpack serve --env development --open",
    "serve": "http-server dist",
    "test": "npx jest",
    "test:watch": "npx jest -o --watch",
    "test:coverage": "npx jest --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "{,src}/**/*.{ts,js}": [
      "eslint --fix"
    ]
  },
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "gh-pages": "^4.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.3.1",
    "jest-localstorage-mock": "^2.4.22",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "string-replace-loader": "^3.1.0",
    "ts-jest": "^29.0.2",
    "ts-loader": "^9.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.8.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "dependencies": {
    "ayaka": "file:../ayaka/build",
    "lit-element": "^2.5.1"
  }
}
